<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
<Project>
		<Option title="app.axis" />
		<Option pch_mode="2" />
  		<Option compiler="gcc" />
		<Option extended_obj_names="1" />
		<Build>
			<Target title="basis">
				<Option output="$(#time_dir)/x64/$(TARGET_NAME)/$(PROJECT_NAME).axis" prefix_auto="0" extension_auto="0" />
				<Option working_dir="$(#time_dir)/x64/$(TARGET_NAME)/" />
				<Option object_output="$(#time_dir)/intermediate/platform/app.axis" />
				<Option type="1" />
		 		<Option compiler="gcc" />
				<Option use_console_runner="0" />
				<Option parameters=": app=app-core/hellomultiverse build_number=basis locale=_std schema=_std" />
				<Option host_application="$(#time_dir)/x64/$(TARGET_NAME)/$(PROJECT_NAME)" />
				<Option run_host_application_in_terminal="0" />
				<Compiler>
					<Add option="-pg" />
					<Add option="-g" />
					<Add option="-D_DEBUG" />
				</Compiler>
				<Linker>
					<Add option="-pg" />
				</Linker>
			</Target>
			<Target title="memdleak">
				<Option output="$(#time_dir)/x64/$(TARGET_NAME)/$(PROJECT_NAME).axis" prefix_auto="0" extension_auto="0" />
				<Option working_dir="$(#time_dir)/x64/$(TARGET_NAME)/" />
				<Option object_output="$(#time_dir)/intermediate/platform/app.axis" />
				<Option type="0" />
		 		<Option compiler="gcc" />
				<Option parameters=": app=app-core/hellomultiverse build_number=basis locale=_std schema=_std" />
				<Option host_application="$(#time_dir)/x64/$(TARGET_NAME)/$(PROJECT_NAME)" />
				<Option run_host_application_in_terminal="0" />
				<Compiler>
					<Add option="-g" />
					<Add option="-D_DEBUG" />
					<Add option="-DMEMDLEAK" />
				</Compiler>
			</Target>
		</Build>
		<Compiler>
			<Add option="-std=c++0x" />
			<Add option="-fexceptions" />
			<Add option="-fnon-call-exceptions" />
			<Add option="-fno-pie" />
			<Add option="-fno-stack-protector" />
			<Add option="-Wno-pointer-arith" />
			<Add option="-Wno-attributes" />
			<Add option="-Winvalid-pch" />
			<Add option="-Wno-conversion-null" />
			<Add option="-fPIC" />
			<Add directory="." />
			<Add directory="$(PROJECT_DIR)" />
			<Add directory="$(#archive)" />
			<Add directory="$(#config)" />
			<Add directory="$(#config)" />
			<Add directory="$(WORKSPACE_DIR)/../.." />
			<Add directory="$(WORKSPACE_DIR)/../../inc" />
			<Add directory="$(WORKSPACE_DIR)/../../include" />
			<Add directory="$(WORKSPACE_DIR)/../../platform-freebsd" />
			<Add directory="$(WORKSPACE_DIR)/../../app/_include" />
			<Add directory="$(WORKSPACE_DIR)/../../app" />

			<Add directory="$(#sensitive)/sensitive/include" />
		</Compiler>
		<Linker>
			<Add option="-Wl,-rpath=\\$$$ORIGIN" />
			
			
			<Add option="-lX11" />
			<Add option="-latomiconv" />
			<Add option="-lsqlite3" />
			<Add option="-laura" />
			<Add option="-lbz2" />
			<Add option="-lz" />
			<Add option="-lcrypto" />
			<Add option="-lssl" />
			<Add option="-lidn" />
			<Add option="-laxis" />
			<Add option="-Wl,-z,defs" />
			<Add directory="$(#time_dir)/x64/$(TARGET_NAME)" />
			<Add directory="/usr/lib64/mysql/" />
		</Linker>
		<Unit filename="app.axis.cpp" />
		<Unit filename="framework.h">
			<Option compile="1" />
			<Option weight="0" />
		</Unit>
		<Extensions>
			<code_completion />
			<debugger>
				<search_path add="/ca2/app/appseed/ca/" />
			</debugger>
			<envvars />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
