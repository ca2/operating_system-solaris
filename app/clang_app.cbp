		<VirtualTargets>
			<Add alias="all_sanitizers" targets="basis;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-fPIC" />
			<Add option="-std=c++17" />
			<Add option='-include &quot;framework.h&quot;' />
