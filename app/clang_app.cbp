					<Add option="-O0" />
					<Add option="-std=c++17" />
					<Add option="-pg" />
					<Add option="-fPIC" />
					<Add option="-g" />
				</Compiler>
				<Linker>
					<Add option="-pg" />
				</Linker>
			</Target>
			<Target title="memdleak">
				<Option output="$(#time_dir)/x64/$(TARGET_NAME)/$(PROJECT_NAME)" prefix_auto="0" extension_auto="0" />
				<Option working_dir="$(#time_dir)/x64/$(TARGET_NAME)/" />
