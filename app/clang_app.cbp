<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="app" />
		<Option pch_mode="2" />
		<Option compiler="clang" />
		<Option extended_obj_names="1" />
		<Build>
			<Target title="basis">
